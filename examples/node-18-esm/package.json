{
  "name": "node-18-esm-example",
  "version": "0.0.0",
  "private": true,
  "description": "Sushi Node18 ESM Example",
  "keywords": [
    "sushi",
    "config",
    "hardhat"
  ],
  "homepage": "https://www.sushi.com",
  "repository": {
    "type": "git",
