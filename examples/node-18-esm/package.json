    "@sushiswap/prettier-config": "workspace:*",
    "@sushiswap/typescript-config": "workspace:*",
    "@types/node": "^16.0.0",
    "eslint": "8.29.0",
    "tsup": "6.5.0",
    "typescript": "4.9.4"
  },
  "engines": {
    "node": ">=16.x"
